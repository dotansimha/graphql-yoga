{
  "name": "@graphql-yoga/common",
  "version": "2.5.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/common"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "generate-graphiql-html": "node scripts/generate-graphiql-html.js",
    "prepack": "bob prepack",
    "check": "tsc --pretty --noEmit"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "@envelop/core": "^2.0.0",
    "@envelop/disable-introspection": "^3.0.0",
    "@envelop/parser-cache": "^4.0.0",
    "@envelop/validation-cache": "^4.0.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@graphql-tools/schema": "^8.3.1",
    "@graphql-tools/utils": "^8.6.0",
    "@graphql-yoga/subscription": "2.0.0",
    "cross-undici-fetch": "^0.4.2",
    "dset": "^3.1.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@minify-html/js": "0.8.0",
    "bob-the-bundler": "^1.5.1"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  }
}
