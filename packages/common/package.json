{
  "name": "@graphql-yoga/common",
  "version": "2.8.3",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/common"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "generate-graphiql-html": "node scripts/generate-graphiql-html.js",
    "check": "tsc --pretty --noEmit"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "@envelop/core": "^2.0.0",
    "@envelop/parser-cache": "^4.0.0",
    "@envelop/validation-cache": "^4.0.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@graphql-tools/schema": "^8.3.1",
    "@graphql-tools/utils": "^8.6.0",
    "@graphql-yoga/subscription": "2.0.0",
    "cross-undici-fetch": "^0.4.2",
    "dset": "^3.1.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "bob-the-bundler": "2.0.0-canary-57a6054.0",
    "html-minifier-terser": "7.0.0-beta.0"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  },
  "type": "module"
}
