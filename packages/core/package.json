{
  "name": "@graphql-yoga/core",
  "version": "0.2.0-alpha.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/core"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "prepack": "bob prepack"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "@envelop/core": "^1.6.2",
    "@envelop/disable-introspection": "^1.2.1",
    "@envelop/parser-cache": "^2.2.1",
    "@envelop/validation-cache": "^2.2.1",
    "@graphql-tools/schema": "8.3.1",
    "@graphql-yoga/handler": "0.2.0-alpha.0",
    "ts-log": "^2.2.4"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "bob-the-bundler": "^1.5.1"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  }
}
