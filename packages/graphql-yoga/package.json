{
  "name": "graphql-yoga",
  "version": "2.13.4",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/graphql-yoga"
  },
  "main": "dist/_cjs/index.js",
  "module": "dist/_esm/index.js",
  "bin": {
    "gql-yoga": "dist/_cjs/cli/bin.js",
    "graphql-yoga": "dist/_cjs/cli/bin.js",
    "yoga": "dist/_cjs/cli/bin.js"
  },
  "scripts": {
    "prepack": "bob prepack",
    "generate-graphiql-html": "node scripts/generate-graphiql-html.js",
    "check": "tsc --pretty --noEmit"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/_cjs/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/_esm/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/_esm/index.js"
      }
    },
    "./schema": {
      "require": {
        "types": "./dist/schema.d.ts",
        "default": "./dist/_cjs/schema.js"
      },
      "import": {
        "types": "./dist/schema.d.ts",
        "default": "./dist/_esm/schema.js"
      },
      "default": {
        "types": "./dist/schema.d.ts",
        "default": "./dist/_esm/schema.js"
      }
    },
    "./subscription": {
      "require": {
        "types": "./dist/subscription.d.ts",
        "default": "./dist/_cjs/subscription.js"
      },
      "import": {
        "types": "./dist/subscription.d.ts",
        "default": "./dist/_esm/subscription.js"
      },
      "default": {
        "types": "./dist/subscription.d.ts",
        "default": "./dist/_esm/subscription.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "@graphql-tools/code-file-loader": "^7.3.0",
    "@graphql-tools/mock": "^8.7.0",
    "@envelop/core": "^2.4.1",
    "@envelop/parser-cache": "^4.4.0",
    "@envelop/validation-cache": "^4.4.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@graphql-tools/schema": "^8.5.0",
    "@graphql-tools/utils": "^8.8.0",
    "@graphql-yoga/subscription": "^2.2.2",
    "@whatwg-node/fetch": "^0.2.3",
    "@whatwg-node/server": "^0.0.5",
    "dset": "^3.1.1",
    "graphql-config": "^4.1.0",
    "tslib": "^2.3.1",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@types/node": "16.11.46",
    "html-minifier-terser": "7.0.0-beta.0",
    "@envelop/disable-introspection": "^3.4.0",
    "@envelop/live-query": "^3.4.0",
    "@types/eventsource": "1.1.9",
    "eventsource": "2.0.2",
    "graphql": "^16.0.1",
    "graphql-scalars": "1.17.0",
    "puppeteer": "15.5.0",
    "@types/yargs": "17.0.10"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  },
  "type": "module"
}
