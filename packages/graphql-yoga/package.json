{
  "name": "graphql-yoga",
  "version": "3.0.0-next.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/graphql-yoga"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "bin": {
    "gql-yoga": "dist/cjs/cli/bin.js",
    "graphql-yoga": "dist/cjs/cli/bin.js",
    "yoga": "dist/cjs/cli/bin.js"
  },
  "scripts": {
    "prepack": "bob prepack",
    "generate-graphiql-html": "node scripts/generate-graphiql-html.js",
    "check": "tsc --pretty --noEmit"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "@graphql-tools/code-file-loader": "^7.3.0",
    "@graphql-tools/mock": "^8.7.0",
    "@graphql-tools/utils": "8.12.0",
    "@envelop/core": "^2.5.0",
    "@envelop/parser-cache": "^4.6.0",
    "@envelop/validation-cache": "^4.6.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@graphql-tools/schema": "^9.0.0",
    "@graphql-yoga/subscription": "^3.0.0-next.0",
    "@whatwg-node/fetch": "0.4.5",
    "@whatwg-node/server": "0.4.9",
    "dset": "^3.1.1",
    "graphql-config": "^4.1.0",
    "tslib": "^2.3.1",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@types/node": "16.11.60",
    "html-minifier-terser": "7.0.0",
    "@envelop/disable-introspection": "^3.5.0",
    "@envelop/live-query": "^4.1.0",
    "graphql": "^16.0.1",
    "graphql-scalars": "1.18.0",
    "graphql-http": "^1.6.0",
    "puppeteer": "18.0.5",
    "@types/yargs": "17.0.13"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  },
  "type": "module",
  "sideEffects": false
