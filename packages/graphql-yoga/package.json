{
  "name": "graphql-yoga",
  "version": "2.0.0-beta.8",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/graphql-yoga"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "bin": {
    "gql-yoga": "dist/bin.js",
    "graphql-yoga": "dist/bin.js",
    "yoga": "dist/bin.js"
  },
  "scripts": {
    "prepack": "bob prepack",
    "check": "tsc --pretty --noEmit"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "buildOptions": {
    "input": "./src/index.ts",
    "bin": {
      "graphql-yoga": {
        "input": "src/bin.ts"
      }
    }
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "@graphql-tools/code-file-loader": "^7.2.3",
    "@graphql-tools/mock": "^8.6.3",
    "@graphql-yoga/node": "0.1.0-beta.8",
    "graphql-config": "^4.1.0",
    "tslib": "^2.3.1",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@types/yargs": "17.0.10"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  }
}
