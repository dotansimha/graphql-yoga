{
  "name": "graphql-yoga",
  "engines": {
    "node": ">=16.0.0"
  },
  "version": "3.9.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/graphql-yoga"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "prepack": "bob prepack",
    "generate-graphiql-html": "node scripts/generate-graphiql-html.js",
    "check": "tsc --pretty --noEmit",
    "postinstall": "node node_modules/puppeteer/install.js"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "@envelop/core": "^4.0.0",
    "@graphql-tools/executor": "^1.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-tools/utils": "^10.0.0",
    "@graphql-yoga/subscription": "^3.1.0",
    "@graphql-yoga/logger": "^0.0.1",
    "@whatwg-node/fetch": "^0.9.0",
    "@whatwg-node/server": "^0.8.1",
    "dset": "^3.1.1",
    "lru-cache": "^9.0.0",
    "tslib": "^2.5.2"
  },
  "devDependencies": {
    "@envelop/disable-introspection": "5.0.0",
    "@envelop/live-query": "6.0.0",
    "@graphql-yoga/render-graphiql": "3.9.1",
    "@jest/globals": "^29.2.1",
    "@n1ru4l/in-memory-live-query-store": "0.10.0",
    "@repeaterjs/repeater": "^3.0.4",
    "@types/node": "18.16.16",
    "graphql": "^16.0.1",
    "graphql-http": "^1.18.0",
    "graphql-scalars": "1.22.1",
    "graphql-sse": "2.1.3",
    "html-minifier-terser": "7.2.0",
    "json-bigint-patch": "0.0.8",
    "puppeteer": "19.6.0"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  },
  "type": "module",
  "sideEffects": false
}
