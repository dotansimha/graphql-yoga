{
  "name": "graphql-yoga",
  "version": "2.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/graphql-yoga"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "prepack": "bob prepack"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "@envelop/core": "^1.3.0",
    "@graphql-yoga/core": "0.1.0",
    "@graphql-yoga/handler": "0.1.0",
    "fastify": "^3.22.1",
    "graphql": "^15.6.1",
    "graphql-helix": "^1.8.3",
    "pino": "^7.0.5",
    "pino-pretty": "^7.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.1",
    "bob-the-bundler": "^1.5.1"
  }
}
