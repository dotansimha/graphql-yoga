{
  "name": "graphql-yoga",
  "version": "2.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/graphql-yoga"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "prepack": "bob prepack"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "@ardatan/graphql-helix": "0.0.6",
    "@envelop/core": "^1.6.2",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@graphql-yoga/core": "workspace:*",
    "@graphql-yoga/handler": "workspace:*",
    "cross-undici-fetch": "^0.1.4",
    "fastify": "^3.24.0",
    "fastify-cors": "^6.0.2",
    "pino": "^7.2.0",
    "pino-pretty": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "bob-the-bundler": "^1.5.1",
    "graphql": "^16.0.1"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  }
}
