{
  "name": "@graphql-yoga/node",
  "version": "2.13.4",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/node"
  },
  "main": "dist/_cjs/index.js",
  "module": "dist/_esm/index.js",
  "scripts": {
    "check": "tsc --pretty --noEmit"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/_cjs/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/_esm/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/_esm/index.js"
      }
    },
    "./schema": {
      "require": {
        "types": "./dist/schema.d.ts",
        "default": "./dist/_cjs/schema.js"
      },
      "import": {
        "types": "./dist/schema.d.ts",
        "default": "./dist/_esm/schema.js"
      },
      "default": {
        "types": "./dist/schema.d.ts",
        "default": "./dist/_esm/schema.js"
      }
    },
    "./subscription": {
      "require": {
        "types": "./dist/subscription.d.ts",
        "default": "./dist/_cjs/subscription.js"
      },
      "import": {
        "types": "./dist/subscription.d.ts",
        "default": "./dist/_esm/subscription.js"
      },
      "default": {
        "types": "./dist/subscription.d.ts",
        "default": "./dist/_esm/subscription.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "graphql-yoga": "2.13.4",
    "tslib": "^2.3.1"
  },
  "type": "module"
}
