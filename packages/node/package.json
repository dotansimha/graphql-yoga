{
  "name": "@graphql-yoga/node",
  "version": "2.13.13",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/node"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "check": "tsc --pretty --noEmit"
  },
  "keywords": [
    "graphql",
    "server",
    "api",
    "graphql-server"
  ],
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.cts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "@envelop/core": "^2.5.0",
    "@graphql-tools/utils": "^8.8.0",
    "@graphql-yoga/common": "^2.12.12",
    "@graphql-yoga/subscription": "^2.2.3",
    "@whatwg-node/fetch": "^0.4.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@envelop/disable-introspection": "3.6.0",
    "@envelop/live-query": "4.2.0",
    "@types/eventsource": "1.1.9",
    "@types/node": "16.11.68",
    "eventsource": "2.0.2",
    "graphql": "^16.0.1",
    "graphql-scalars": "1.18.0",
    "puppeteer": "18.2.1"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  },
  "type": "module"
}
