{
  "name": "@graphql-yoga/subscription",
  "version": "5.0.4",
  "type": "module",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-hive/graphql-yoga.git",
    "directory": "packages/subscription"
  },
  "author": "Laurin Quast <laurinquast@googlemail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.cts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "keywords": [
    "pubsub",
    "graphql",
    "event",
    "subscription"
  ],
  "scripts": {
    "check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@graphql-yoga/typed-event-target": "workspace:^",
    "@repeaterjs/repeater": "^3.0.4",
    "@whatwg-node/events": "^0.1.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@graphql-yoga/redis-event-target": "workspace:*",
    "@types/ioredis-mock": "8.2.5",
    "@whatwg-node/server": "^0.10.2",
    "ioredis-mock": "8.9.0"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": true,
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
