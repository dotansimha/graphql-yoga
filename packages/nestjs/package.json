{
  "name": "@graphql-yoga/nestjs",
  "version": "0.3.1",
  "description": "GraphQL Yoga driver for NestJS GraphQL.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/nestjs"
  },
  "author": "Denis Badurina <badurinadenis@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "check": "tsc --pretty --noEmit"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.cts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "files": [
    "LICENSE.md",
    "lib",
    "README.md"
  ],
  "keywords": [
    "graphql",
    "server",
    "nestjs",
    "nest",
    "driver",
    "graphql-yoga"
  ],
  "peerDependencies": {
    "@apollo/gateway": "^2.0.0",
    "@apollo/subgraph": "^2.0.0",
    "@envelop/apollo-federation": "^3.0.0",
    "@envelop/core": "^3.0.0",
    "@nestjs/common": "^8.4.7 || ^9.0.0",
    "@nestjs/core": "^8.4.7 || ^9.0.0",
    "@nestjs/graphql": "^10.0.0",
    "graphql-yoga": "^3.0.0",
    "graphql": "^15.0.0 || ^16.0.0"
  },
  "peerDependenciesMeta": {
    "@apollo/gateway": {
      "optional": true
    },
    "@apollo/subgraph": {
      "optional": true
    },
    "@envelop/apollo-federation": {
      "optional": true
    },
    "@envelop/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@apollo/gateway": "^2.3.2",
    "@apollo/subgraph": "^2.3.2",
    "@envelop/apollo-federation": "^3.0.5",
    "@envelop/core": "^3.0.6",
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/platform-fastify": "^9.3.9",
    "@nestjs/testing": "^9.3.9",
    "@swc/core": "^1.3.35",
    "@theguild/eslint-config": "^0.7.0",
    "@theguild/prettier-config": "^1.1.0",
    "@types/eslint": "^8.21.1",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.0.1",
    "@types/ws": "^8.5.4",
    "@whatwg-node/fetch": "^0.8.1",
    "eslint": "^8.32.0",
    "express": "^4.18.2",
    "fastify": "^4.13.0",
    "glob": "^8.1.0",
    "graphql": "^16.6.0",
    "graphql-http": "^1.16.0",
    "graphql-ws": "^5.11.3",
    "graphql-yoga": "^3.7.0",
    "prettier": "^2.8.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "subscriptions-transport-ws": "^0.11.0",
    "tslib": "^2.5.0",
    "typescript": "4.9.4",
    "ws": "^8.12.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
