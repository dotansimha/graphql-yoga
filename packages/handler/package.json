{
  "name": "@graphql-yoga/handler",
  "version": "0.2.0-alpha.1",
  "description": "Normalize requests and response",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-yoga.git",
    "directory": "packages/handler"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "prepack": "bob prepack"
  },
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://github.com/saihaj)",
  "license": "MIT",
  "devDependencies": {
    "@envelop/core": "^1.6.1",
    "@types/node": "^16.11.7",
    "bob-the-bundler": "^1.5.1",
    "typescript": "^4.4.4"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "@ardatan/graphql-helix": "^0.0.8",
    "cross-undici-fetch": "^0.1.4"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  }
}
