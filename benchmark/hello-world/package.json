    "express-graphql": "0.12.0",
    "fastify": "3.25.0",
    "graphql": "16.1.0",
    "graphql-yoga": "2.0.0-alpha.3",
    "mercurius": "9.0.0",
    "wait-on": "6.0.0"
  }
}
